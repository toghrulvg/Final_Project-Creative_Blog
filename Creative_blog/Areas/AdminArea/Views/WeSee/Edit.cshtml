@model WeSee
@{
    ViewData["Title"] = "Create";
}



<div class="container-fluid mx-5 my-3">
    <h3>WeSee Edit</h3>
    <form asp-action="Edit" method="post" enctype="multipart/form-data">
        <div class="form-group">

            <label for="Icon">Icon</label>
            <input class="form-control" asp-for="Icon">
            <span class="text-danger" asp-validation-for="Icon"></span>

            <label for="Name">Name</label>
            <input class="form-control" asp-for="Name">
            <span class="text-danger" asp-validation-for="Name"></span>



        </div>
        <a class="btn btn-primary" asp-controller="WeSee" asp-action="Index">Back</a>
        <a class="btn btn-primary" asp-controller="WeSee" asp-action="Create">Reset</a>
        <button type="submit" class="btn btn-primary">Edit</button>
    </form>
</div>


